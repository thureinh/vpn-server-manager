  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "aws-sdk": "^2.1632.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "graphql": "^16.8.1",
    "graphql-http": "^1.22.0",
    "graphql-middleware": "^6.1.35",
    "graphql-shield": "^7.6.5",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nexus": "^1.3.0"
  },
  "devDependencies": {
    "prisma": "^5.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
